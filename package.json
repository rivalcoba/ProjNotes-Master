{
  "name": "expweb-bp",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "cross-env NODE_ENV=production node ./dist/bin/www",
    "dev": "cross-env NODE_ENV=development babel-watch --watch ./server/views ./server/bin/www",
    "w:s": "cross-env NODE_ENV=development webpack serve --config ./webpack.dev.config.js --mode development",
    "cleans": "cross-env rimraf dist",
    "cleanc": "cross-env rimraf public/js && rimraf public/styles",
    "b:s": "cross-env npm run cleans && babel ./server --out-dir dist --copy-files",
    "b:c": "cross-env npm run cleanc && webpack --config ./webpack.config.js --mode production",
    "build": "cross-env npm run b:c && npm run b:s",
    "clnlogs": "cross-env rimraf server/logs/*.log"
  },
  "dependencies": {
    "app-root-path": "^3.0.0",
    "bcryptjs": "^2.4.3",
    "connect-flash": "^0.1.1",
    "connect-mongo": "^4.6.0",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "express": "~4.16.1",
    "express-handlebars": "^5.3.0",
    "express-session": "^1.17.2",
    "handlebars": "^4.7.6",
    "hbs": "^4.1.1",
    "http-errors": "~1.6.3",
    "method-override": "^3.0.0",
    "mongoose": "^5.12.5",
    "morgan": "~1.9.1",
    "nodemailer": "^6.8.0",
    "passport": "^0.6.0",
    "passport-local": "^1.0.0",
    "randomstring": "^1.2.3",
    "rimraf": "^2.7.1",
    "validator": "^13.7.0",
    "webpack": "^5.24.0",
    "webpack-dev-middleware": "^4.1.0",
    "webpack-hot-middleware": "^2.25.0",
    "winston": "^3.3.3",
    "yup": "^0.32.9"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.17",
    "@babel/core": "^7.12.17",
    "@babel/node": "^7.12.17",
    "@babel/preset-env": "^7.12.17",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-preset-minify": "^0.5.1",
    "babel-watch": "^7.4.0",
    "core-js": "^3.9.0",
    "cross-env": "^7.0.3",
    "css-loader": "^5.0.2",
    "eslint": "^7.21.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-import-resolver-babel-module": "^5.3.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-webpack-plugin": "^2.5.2",
    "mini-css-extract-plugin": "^1.3.8",
    "prettier": "^2.2.1",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "^3.11.2"
  },
  "engines": {
    "node": "16.19.0"
  }
}
